<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bastian.eventosusach">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.login" />
        <activity android:name=".views.register" />
        <activity android:name=".views.DescEventView" />
        <activity android:name=".views.perfilUsuario" />
        <activity android:name=".views.ItemDetail" />
        <activity android:name=".views.ItemList"></activity>
    </application>

</manifest>